#print integer
print(20)

#print float
print(3.5)

#print string
print("hello")

#print string
print('hello')

#print series of string using '+'
print("hello"+"world")

#string concatination: printing non string number as string
print("my number is "+ str(987654321)+ " this is my mobile number")

#to print multiple values
print(1, 20, 3.5, "hello")
